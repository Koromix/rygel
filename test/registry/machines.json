{
    "debian_arm32": {
        "name": "Debian ARM32",

        "qemu": {
            "binary": "qemu-system-arm",
            "arguments": ["-M", "virt", "-cpu", "cortex-a15", "-m", "2G", "-smp", 2, "-kernel", "vmlinuz-5.10.0-12-armmp-lpae", "-initrd", "initrd.img-5.10.0-12-armmp-lpae", "-append", "root=/dev/vda2 apparmor=0", "-drive", "if=none,file=disk.qcow2,format=qcow2,id=hd", "-device", "virtio-blk-device,drive=hd", "-netdev", "user,id=mynet,hostfwd=tcp::22201-:22", "-device", "virtio-net-device,netdev=mynet", "-vnc", "127.0.0.1:11"]
        },

        "info": {
            "version": 3,
            "platform": "linux",
            "arch": "arm",

            "ssh_port": 22201,
            "vnc_port": 5911,

            "username": "debian",
            "password": "debian",

            "shutdown": "sudo poweroff"
        },

        "tests": {
            "Linux ARM32": {
                "directory": "/home/debian/luigi",
                "build": "npm install --production",
                "commands": {
                    "Test Raylib": "xvfb-run node test/tests/raylib.js",
                    "Test Misc": "node test/tests/misc.js",
                    "Test SQLite": "node test/tests/sqlite.js"
                }
            }
        }
    },

