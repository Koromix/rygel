[Goupile]
BinaryDirectory = .
DomainDirectory = domains
RunUser = {{goupile_run_user}}

DefaultAdmin = {{goupile_default_username}}
DefaultPassword = {{goupile_default_password}}

[NGINX]
ConfigDirectory = nginx/domains.d
ServerInclude = nginx/include.conf
ServiceName = {{nginx_service_name}}

[Domains]
{% for domain in goupile_domains -%}
    {{ domain }}
{% endfor %}
