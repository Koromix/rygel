  "files": [
    "src",
    "doc",
    "benchmark/CMakeLists.txt",
    "benchmark/atoi_*",
    "benchmark/raylib_*",
    "qemu/qemu.js",
    "qemu/registry",
    "test/async.js",
    "test/callbacks.js",
    "test/CMakeLists.txt",
    "test/misc.c",
    "test/misc.def",
    "test/raylib.js",
    "test/sqlite.js",
    "test/sync.js",
    "vendor",
    "LICENSE.txt",
    "README.md",
    "ChangeLog.md",
    "CMakeLists.txt",
    "build/qemu/*/*.tar.gz"
  ]
